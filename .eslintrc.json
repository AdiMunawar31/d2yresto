{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "import/order": 0,
        "no-prototype-builtins": 0,
        "consistent-return": 0,
        "no-new": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0,
        "import/prefer-default-export": 0,
        "no-empty-function": 0,
        "no-unused-vars": 0,
        "no-useless-return": 0,
        "no-undef": 0
    }
}
